{
  "compilerOptions": {
    "jsx": "react-native",
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "noImplicitReturns": true,
    "noImplicitThis": true,
    "noImplicitAny": true,
    "strictNullChecks": true,
    "paths":{
      "@auth":["./src/auth/"],
      "@components/*": ["./src/components/*"],
      "@reduxStore/*":["./src/ReduxStore/*"],
      "@Screens":["./src/Screens/"],
      "@theme":["./src/theme.ts"],
      "@types": ["./src/types.ts"],
      "@utility/*": ["./src/Utility/*"],
      "@app": ["./src/RouteApp"],
    }
  },
  "extends": "expo/tsconfig.base",
  "include": [
    "src/**/*.ts",
    "src/**/*.tsx"
  ],
  "exclude": [
    "node_modules",
    "babel.config.js",
    "metro.config.js",
    "jest.config.js"
  ],
  "parser": "typescript-eslint-parse"
}
